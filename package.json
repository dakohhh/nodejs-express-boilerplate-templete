{
  "name": "boiler-plate-template",
  "version": "1.0.0",
  "description": "<bolierplate description>",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./build/src/index.js",
    "dev": "cross-env ENV=development nodemon --exec ts-node -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc && tsc-alias",
    "lint": "eslint \"{src,apps,libs,test}/**/*{.ts,.mjs}\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*{.ts}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,css,md,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,css,md,tsx}\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.668.0",
    "@aws-sdk/cloudfront-signer": "^3.662.0",
    "@aws-sdk/lib-storage": "^3.668.0",
    "@aws-sdk/s3-request-presigner": "^3.668.0",
    "@react-email/components": "0.0.22",
    "base64url": "^3.0.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.5.1",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.5",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
